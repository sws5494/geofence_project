                        <td>6</td>
                    </tr>

                    <%
                   for(var i=0; i<rows.length; i++)
                   {
                      var oneItem = rows[i];
                      console.log("temp");
              %>
                        <tr align="center">
                            <td>
                                <%=oneItem.identifier%>
                            </td>
                            <td>
                                <%=oneItem.startday%> <%=oneItem.starttime%>
                            </td>
                            <td>
                                <%=oneItem.endday%> <%=oneItem.endtime%>
                            </td>
                            <td>
